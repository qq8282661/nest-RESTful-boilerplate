{
  "name": "nest-typescript-starter",
  "version": "1.0.0",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "start": "export NODE_ENV='production'&& node src/main.js",
    "dev": "find ./src  -name '*.js' -exec rm {} \\; && npm run start:watch",
    "start:watch": "nodemon",
    "start:debug": "nest start --debug --watch",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./e2e/jest-e2e.json",
    "grpc:gen": "./node_modules/.bin/rxjs-grpc -o src/grpc/generated.ts src/grpc/protobufs/*.proto",
    "tsc": "nest build"
  },
  "dependencies": {
    "@grpc/proto-loader": "^0.5.4",
    "@nestjs/common": "7.3.2",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "7.3.1",
    "@nestjs/microservices": "^7.3.2",
    "@nestjs/platform-express": "^7.3.2",
    "@nestjs/typeorm": "^7.1.0",
    "alipay-sdk": "^3.1.1",
    "bluebird": "^3.7.2",
    "class-transformer": "0.2.3",
    "class-validator": "0.12.2",
    "fabric-client": "^1.4.10",
    "flake-idgen": "^1.4.0",
    "grpc": "~1.20.3",
    "helmet": "^3.23.3",
    "mysql2": "^2.1.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.5.3",
    "typeorm": "^0.2.25",
    "uuid": "^8.2.0",
    "v8-profiler-node8": "^6.3.0"
  },
  "devDependencies": {
    "@nestjs/cli": "7.4.1",
    "@nestjs/schematics": "7.0.1",
    "@nestjs/testing": "7.3.2",
    "@types/express": "4.17.2",
    "@types/flake-idgen": "^0.1.30",
    "@types/jest": "26.0.4",
    "@types/supertest": "2.0.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^3.9.0",
    "@typescript-eslint/parser": "^3.9.0",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "26.1.0",
    "mwts": "^1.0.7",
    "nodemon": "^2.0.4",
    "prettier": "^1.19.1",
    "rxjs-grpc": "1.0.0-rxjs6.0",
    "supertest": "4.0.2",
    "ts-jest": "26.1.1",
    "ts-loader": "8.0.0",
    "ts-node": "8.10.2",
    "tsconfig-paths": "3.9.0",
    "tslint": "6.1.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.3.0",
    "typescript": "3.9.7"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
